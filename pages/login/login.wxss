.hdr {
  width: 100%;
  color: #938D7B;
  font-size: 36rpx;
  font-weight: 600;
  padding: 20rpx 30rpx;
  border-radius: 10rpx;
  box-sizing: border-box;
  background-color: #FDF8E5;
  border: 2rpx solid #F3EFDD;
}

.ul {
  width: 100%;
  margin: 25rpx 0;
  position: relative;
}

.li {
  width: 100%;
  height: 80rpx;
  color: #aaaaaa;
  position: relative;
  text-align: center;
}

.li > text {
  height: 80rpx;
  padding: 20rpx;
  font-size: 30rpx;
  line-height: 40rpx;
  vertical-align: top;
  display: inline-block;
  box-sizing: border-box;
}

.li::before {
  content: '';
  width: 40rpx;
  height: 80rpx;
  padding: 25rpx 0;
  vertical-align: top;
  display: inline-block;
  box-sizing: border-box;
  background-repeat: no-repeat;
  background-size: 30rpx 30rpx;
  background-position: center center;
  background-image: url("https://noname.wodemo.net/down/484407/checked_gray.png");
}